{
  "parser": "@typescript-eslint/parser",
  "env": {
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": [
    "oclif",
    "oclif-typescript",
    "standard",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module",
    "project": "tsconfig.json"
  },
  "plugins": [
    "@typescript-eslint",
    "jest",
    "header"
  ],
  "rules": {
    "@typescript-eslint/no-unnecessary-type-arguments": "error",
    "@typescript-eslint/no-unnecessary-type-assertion": "error",
    "node/shebang": "error"
  }
}

